{
  "manifest_version": 2,
  "name": "Search Interceptor",
  "version": "1.0",
  "description": "Intercepts searches made from the URL bar",
  "permissions": ["webNavigation", "tabs", "search", "<all_urls>"],
  "omnibox": { "keyword": "!" },
  "background": {
    "scripts": ["bangs.js", "background.js"],
    "persistent": false
  }
}
